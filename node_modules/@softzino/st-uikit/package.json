{
  "name": "@softzino/st-uikit",
  "version": "0.0.13",
  "description": "A Vue 3 Component Library. Fairly Complete, Theme Customizable, Uses TypeScript, Fast",
  "type": "module",
  "main": "./dist/st-uikit.umd.cjs",
  "module": "./dist/st-uikit.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/st-uikit.js",
      "require": "./dist/st-uikit.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:watch": "vue-tsc && vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest --dom"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.14.2",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.0",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "happy-dom": "^14.12.0",
    "postcss": "^8.4.38",
    "sortablejs": "^1.15.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/softzino/st-uikit"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "st-uikit",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.4",
    "@popperjs/core": "^2.11.8",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^3.6.0",
    "luxon": "^3.5.0",
    "tailwind-merge": "^2.3.0"
  }
}
